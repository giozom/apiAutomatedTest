{
  "name": "typicode-api-verification",
  "version": "1.0.0",
  "description": "API Verification for Typicode running locally",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --reporter mochawesome",
    "test:get": "./node_modules/mocha/bin/mocha --grep @get --reporter mochawesome",
    "test:post": "./node_modules/mocha/bin/mocha --grep @post --reporter mochawesome",
    "test:put": "./node_modules/mocha/bin/mocha --grep @put --reporter mochawesome",
    "test:delete": "./node_modules/mocha/bin/mocha --grep @delete --reporter mochawesome",
    "ciTest": "NODE_ENV=ci multi='spec=- mocha-junit-reporter=test-results.xml' ./node_modules/mocha/bin/mocha --reporter mocha-multi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giozom/apiAutomatedTest"
  },
  "license": "ISC",
  "dependencies": {
    "chai": "3.5.0",
    "chai-subset": "1.2.0",
    "chai-things": "0.2.0",
    "konfig": "0.2.1",
    "leche": "2.1.1",
    "lodash": "4.17.19",
    "messagecheckr": "2.1.3",
    "mocha": "2.4.5",
    "mocha-junit-reporter": "1.9.1",
    "mocha-multi": "0.7.2",
    "mochawesome": "1.4.0",
    "q": "1.4.1",
    "redis": "2.4.2",
    "request": "2.69.0",
    "substor": "1.0.0",
    "supertest": "1.1.0",
    "supertest-as-promised": "2.0.2",
    "validator": "4.7.1",
    "xmldoc": "0.3.2"
  },
  "devDependencies": {
    "mochawesome": "^1.4.0",
    "moment": "^2.14.1",
    "supertest": "^1.1.0"
  }
}
